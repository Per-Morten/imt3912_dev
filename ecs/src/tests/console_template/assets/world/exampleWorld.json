{
	"actors": // This is the list of all actors in the world.
	[
		{
			"extend": "MovingActor", // All properties will be extended from MovingActor
			"components": // We can override values in the components.
			{
				"Physics":
				{
					// This will override the specified values in MovingActor, but
					// all other values will not be changed.
					"linearVelocity": {"x": 1.0, "y": 0.5},
					"angularVelocity": 10
				}
			}
		}
	]
}
